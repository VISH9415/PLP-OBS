function alertLogin(){
	alert("Successfully logged in !");
	return true;
	} 

function alertLoginF(){
	alert("login failed!");
	return true;
	} 

function isNumberKey(evt){
    var charCode = (evt.which) ? evt.which : event.keyCode
    if (charCode > 31 && (charCode < 48 || charCode > 57))
        return false;
    return true;
}